; test copy/load/store
; pc should end at 0xA

	.text
  lex $1, 0xFF
  lex $2, 0x00
  store $1, $2 ; store 0xFFFF at location 0
  copy $1, $2 ; copy 0x0000 into reg 1
  brf $1, pass ; halt if any bit of reg 1 is 1
  sys
pass:
  load $1, $2 ; load 0xFFFF into reg 1
  brt $1, pass2; halt if reg 1 is still 0
  sys
pass2:
  sys
